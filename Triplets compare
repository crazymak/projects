#include<stdio.h>
int main()
{
    int arr[3],b[3],i,a=0,bb=0;
    for( i=0;i<3;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<3;i++)
{
    scanf("%d",&b[i]);
}
for(int i=0;i<3;i++)
{
     if (arr[i] > b[i]) 
     a++;
        if (arr[i] < b[i]) 
        bb++;
    }
    printf("%d %d\n", a, bb);
    return 0;
}

